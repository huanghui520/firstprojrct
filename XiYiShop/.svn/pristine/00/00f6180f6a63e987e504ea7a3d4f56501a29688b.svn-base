<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    style="@style/MatchMatch.Vertical"
    android:background="@color/bgcolor">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/mydp59"
        android:layout_gravity="center_vertical"
        android:background="@color/head_line"
        android:gravity="center_vertical">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:text="心驿商家端"
            android:textColor="@color/white"
            android:textSize="@dimen/mydp17" />

        <ImageView
            android:layout_width="@dimen/mydp21"
            android:layout_height="@dimen/mydp16"
            android:layout_alignParentRight="true"
            android:layout_marginRight="@dimen/mydp11"
            android:src="@mipmap/xj" />
    </RelativeLayout>


    <com.cjj.MaterialRefreshLayout
        android:id="@+id/refresh"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:isLoadMore="true">

        <ListView
            android:id="@+id/list_myorder"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />


    </com.cjj.MaterialRefreshLayout>


</LinearLayout>

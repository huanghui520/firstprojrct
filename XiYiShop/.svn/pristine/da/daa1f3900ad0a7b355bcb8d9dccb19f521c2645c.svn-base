<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="#FFFFFF"
    android:orientation="vertical">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingTop="@dimen/mydp17">

        <TextView
            android:id="@+id/tv_money"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/mydp10"
            android:text="提现金额：￥2000"
            android:textColor="#353434"
            android:textSize="@dimen/mysp15" />

        <TextView
            android:id="@+id/tv_shenhe"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_marginRight="@dimen/mydp10"
            android:text="审核中"
            android:textColor="#353434"
            android:textSize="@dimen/mysp15" />

        <TextView
            android:id="@+id/tv_succeed"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_marginRight="@dimen/mydp10"
            android:text="提现成功"
            android:textColor="#12B7F5"
            android:textSize="@dimen/mysp15"
            android:visibility="gone" />

        <TextView
            android:id="@+id/tv_fail"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_marginRight="@dimen/mydp10"
            android:text="失败"
            android:textColor="#DB0505"
            android:textSize="@dimen/mysp15"
            android:visibility="gone" />
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingTop="@dimen/mydp16">

        <TextView
            android:id="@+id/tv_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/mydp10"
            android:text="2016-06-02"
            android:textColor="#545454"
            android:textSize="@dimen/mysp12" />

        <TextView
            android:id="@+id/tv_kahao"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_marginBottom="@dimen/mydp15"
            android:layout_marginRight="@dimen/mydp10"
            android:text="卡号错误"
            android:textColor="#545454"
            android:textSize="@dimen/mysp12" />

    </RelativeLayout>
    <!--<View-->
    <!--android:layout_marginTop="@dimen/mydp15"-->
    <!--android:background="#A0A0A0"-->
    <!--android:layout_width="match_parent"-->
    <!--android:layout_height="@dimen/mydp1"/>-->

</LinearLayout>
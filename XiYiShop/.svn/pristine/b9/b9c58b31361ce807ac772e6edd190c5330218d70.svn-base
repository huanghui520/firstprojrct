            android:textSize="@dimen/mydp15" />

        <ImageView

            android:layout_width="@dimen/mydp12"
            android:layout_height="@dimen/mydp18"
            android:layout_alignParentRight="true"
            android:layout_marginRight="@dimen/mydp9"
            android:clickable="true"

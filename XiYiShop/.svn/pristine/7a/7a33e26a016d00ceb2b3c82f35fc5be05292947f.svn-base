<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#EEEEEE"
    android:orientation="vertical"
    >
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/mydp45"
        android:background="#12B7F5"
        android:gravity="center_vertical"
        android:orientation="horizontal">
        <LinearLayout
            android:id="@+id/iv_back"
            android:layout_width="@dimen/mydp80"
            android:layout_height="match_parent"
            android:gravity="center_vertical"
            >
            <ImageView
                android:layout_width="@dimen/mydp12"
                android:layout_height="@dimen/mydp22"
                android:layout_alignParentLeft="true"
                android:layout_marginLeft="@dimen/mydp11"
                android:clickable="true"
                android:src="@mipmap/left"
                />
        </LinearLayout>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:text="设置"
            android:textColor="#FEFFFF"
            android:textSize="@dimen/mydp17"
            />
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/rl_gaimi"
        android:layout_width="match_parent"
        android:layout_height="@dimen/mydp45"
        android:background="#FFFFFF"
        android:layout_marginTop="@dimen/mydp10"
        android:gravity="center_vertical"
        android:orientation="horizontal">
        <ImageView
            android:layout_width="@dimen/mydp12"
            android:layout_height="@dimen/mydp22"
            android:layout_alignParentRight="true"
            android:layout_marginRight="@dimen/mydp9"
            android:src="@mipmap/right"

            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_centerHorizontal="true"
            android:layout_marginLeft="@dimen/mydp11"
            android:text="修改密码"
            android:textColor="#353434"
            android:textSize="@dimen/mydp15" />
    </RelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/mydp1"
        android:background="#A0A0A0"
        />
    <RelativeLayout
        android:id="@+id/rl_update"
        android:layout_width="match_parent"
        android:layout_height="@dimen/mydp45"
        android:background="#FFFFFF"
        android:gravity="center_vertical"
        android:orientation="horizontal">
        <ImageView
            android:layout_width="@dimen/mydp12"
            android:layout_height="@dimen/mydp22"
            android:layout_alignParentRight="true"
            android:layout_marginRight="@dimen/mydp9"
            android:src="@mipmap/right"

            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_centerHorizontal="true"
            android:layout_marginLeft="@dimen/mydp11"
            android:text="版本更新"
            android:textColor="#353434"
            android:textSize="@dimen/mydp15" />
    </RelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/mydp1"
        android:background="#A0A0A0"
        />
    <RelativeLayout
        android:id="@+id/rl_clear"
        android:layout_width="match_parent"
        android:layout_height="@dimen/mydp45"
        android:background="#FFFFFF"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        >
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_centerHorizontal="true"
            android:layout_marginLeft="@dimen/mydp11"
            android:text="清除缓存"
            android:textColor="#353434"
            android:textSize="@dimen/mydp15" />
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/rl_exit"
        android:layout_width="match_parent"
        android:layout_height="@dimen/mydp45"
        android:layout_marginTop="@dimen/mydp9"
        android:background="#FFFFFF"
        android:gravity="center_vertical"
        android:orientation="horizontal">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_centerHorizontal="true"
            android:layout_marginLeft="@dimen/mydp11"
            android:text="退出登录"
            android:textColor="#353434"
            android:textSize="@dimen/mydp15" />
    </RelativeLayout>

</LinearLayout>

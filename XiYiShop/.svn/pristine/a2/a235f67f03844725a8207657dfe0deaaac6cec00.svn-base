<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/fragment_head"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/head_line"
        android:orientation="vertical">
        <!--账户余额-->
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:orientation="vertical">

            <TextView
                android:layout_marginTop="@dimen/mydp26"
                android:id="@+id/fragment_myorder_balanceNumTxr"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="60086.00"
                android:textColor="@color/white"
                android:textSize="@dimen/mysp46" />

            <TextView
                android:layout_marginBottom="@dimen/mydp26"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/mydp34"
                android:text="账户余额(元)"
                android:textColor="@color/white"
                android:textSize="@dimen/mysp15" />
        </LinearLayout>
        <!--/////////////////////////////////////////////////////////-->
        <!--白色分割线   横向-->
        <View style="@style/all_line" />
        <!-- 三个  使用权重-->
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/mydp85"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="@dimen/mydp0"
                android:layout_height="@dimen/mydp85"
                android:layout_weight="1"
                android:background="#12B7F5"
                android:gravity="center"
                android:orientation="vertical">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="¥ 30000"
                    android:textColor="@color/white"
                    android:textSize="@dimen/mysp23" />

                <TextView
                    style="@style/WrapWrap"
                    android:layout_marginTop="@dimen/mydp8"
                    android:text="总销售额"
                    android:textColor="@color/white"
                    android:textSize="@dimen/mysp14" />
            </LinearLayout>

            <View
                android:layout_width="@dimen/mydp0.5"
                android:layout_height="match_parent"
                android:background="@color/all_line" />

            <LinearLayout
                android:layout_width="@dimen/mydp0"
                android:layout_height="@dimen/mydp85"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="¥ 2160"
                    android:textColor="@color/white"
                    android:textSize="@dimen/mysp23" />

                <TextView
                    style="@style/WrapWrap"
                    android:layout_marginTop="@dimen/mydp8"
                    android:text="今日销售额"
                    android:textColor="@color/white"
                    android:textSize="@dimen/mysp14" />
            </LinearLayout>

            <View
                android:layout_width="@dimen/mydp0.5"
                android:layout_height="match_parent"
                android:background="@color/all_line" />

            <LinearLayout
                android:layout_width="@dimen/mydp0"
                android:layout_height="@dimen/mydp85"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="300"
                    android:textColor="@color/white"
                    android:textSize="@dimen/mysp23" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/mydp8"
                    android:text="今日订单"
                    android:textColor="@color/white"
                    android:textSize="@dimen/mysp14" />
            </LinearLayout>
        </LinearLayout>
        <!--/////////////////////////////////////////////////////////-->

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:background="#FFFFFF"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_width="@dimen/mydp0"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_weight="1">
            />

            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                >
                <ImageView
                    android:id="@+id/rl_my_orderimg"
                    android:layout_width="@dimen/mydp23"
                    android:layout_height="@dimen/mydp29"
                    android:layout_marginLeft="@dimen/mydp52"
                    android:layout_marginTop="@dimen/mydp25"
                    android:src="@mipmap/dindan" />

                <TextView
                    android:layout_width="@dimen/mydp12"
                    android:layout_height="@dimen/mydp12"
                    android:layout_marginLeft="@dimen/mydp71"
                    android:layout_marginTop="@dimen/mydp21"
                    android:background="@drawable/dindan_bg"
                    android:gravity="center"
                    android:text="1"
                    android:textColor="#FFFFFF"
                    android:textSize="@dimen/mysp10" />
            </RelativeLayout>
            <TextView
                android:layout_marginTop="@dimen/mydp21"
                android:layout_gravity="center_horizontal"
                android:text="订单管理"
                android:textColor="#454545"
                android:textSize="@dimen/mysp14"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <ImageView
                android:id="@+id/ll_paymentLin"
                android:layout_marginTop="@dimen/mydp31"
                android:layout_gravity="center"
                android:layout_width="@dimen/mydp26"
                android:layout_height="@dimen/mydp28"
                android:src="@mipmap/fqzf"
                />
            <TextView
                android:layout_marginTop="@dimen/mydp19"
                android:layout_marginBottom="@dimen/mydp30"
                android:layout_gravity="center_horizontal"
                android:text="发起支付"
                android:textColor="#454545"
                android:textSize="@dimen/mysp14"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

        </LinearLayout>

        <LinearLayout
            android:gravity="center_horizontal"
            android:layout_width="@dimen/mydp0"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_weight="1">
            <ImageView
                android:id="@+id/ll_withdraw"
                android:layout_marginTop="@dimen/mydp26"
                android:layout_width="@dimen/mydp28"
                android:layout_height="@dimen/mydp28"
                android:src="@mipmap/tx"
                />
            <TextView
                android:layout_marginTop="@dimen/mydp21"
                android:text="提现"
                android:textColor="#454545"
                android:textSize="@dimen/mysp14"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

            <ImageView
                android:layout_marginTop="@dimen/mydp34"
                android:layout_width="@dimen/mydp32"
                android:layout_height="@dimen/mydp22"
                android:src="@mipmap/ck"
                android:id="@+id/ll_seestoreLin" />

            <TextView
                android:layout_marginTop="@dimen/mydp22"
                android:layout_marginBottom="@dimen/mydp30"
                android:text="商品查看"
                android:textColor="#454545"
                android:textSize="@dimen/mysp14"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

        </LinearLayout>

        <LinearLayout
            android:gravity="center_horizontal"
            android:layout_width="@dimen/mydp0"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_weight="1">
            <ImageView
                android:id="@+id/ll_verifyorderLin"
                android:layout_marginTop="@dimen/mydp25"
                android:layout_width="@dimen/mydp24"
                android:layout_height="@dimen/mydp30"
                android:src="@mipmap/yz"
                />
            <TextView
                android:layout_marginTop="@dimen/mydp20"
                android:text="验证订单"
                android:textColor="#454545"
                android:textSize="@dimen/mysp14"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <ImageView
                android:id="@+id/ll_setLin"
                android:layout_marginTop="@dimen/mydp32"
                android:layout_width="@dimen/mydp28"
                android:layout_height="@dimen/mydp26"
                android:src="@mipmap/szt"
                />
            <TextView
                android:layout_marginTop="@dimen/mydp20"
                android:layout_marginBottom="@dimen/mydp30"
                android:text="设置"
                android:textColor="#454545"
                android:textSize="@dimen/mysp14"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

        </LinearLayout>
    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/mydp10"
        android:background="#EEEEEE"
        />
    <TextView
        android:background="#FFFFFF"
        android:text="订单动态"
        android:textSize="@dimen/mysp14"
        android:textColor="#151515"
        android:gravity="center_vertical"
        android:paddingLeft="@dimen/mydp10"
        android:layout_width="match_parent"
        android:layout_height="@dimen/mydp35" />

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/mydp1"
        android:background="#EEEEEE"
        />

</LinearLayout>





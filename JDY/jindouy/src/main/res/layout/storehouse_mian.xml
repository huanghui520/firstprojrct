<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/main"
    android:orientation="vertical">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/mydp45"
        android:background="#4090FF"
        android:gravity="center_vertical"
        android:orientation="horizontal">
        <LinearLayout
            android:id="@+id/iv_back"
            android:layout_width="@dimen/mydp80"
            android:layout_height="match_parent"
            android:gravity="center_vertical"
            >
            <ImageView
                android:layout_width="@dimen/mydp12"
                android:layout_height="@dimen/mydp22"
                android:layout_alignParentLeft="true"
                android:layout_marginLeft="@dimen/mydp11"
                android:clickable="true"
                />
        </LinearLayout>
        <TextView
            android:layout_centerVertical="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:text="仓库"
            android:textColor="#FFFFFF"
            android:textSize="@dimen/mydp16"
            />
    </RelativeLayout>
    <TextView
        android:textSize="@dimen/mysp29"
        android:layout_marginTop="@dimen/mydp53"
        android:layout_gravity="center_horizontal"
        android:textColor="#999999"
        android:text="快件查询"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <LinearLayout
        android:layout_marginTop="@dimen/mydp43"
        android:layout_gravity="center_horizontal"
        android:layout_width="@dimen/mydp301"
        android:layout_height="@dimen/mydp40"
        android:background="@drawable/bg_select_ss"
        android:orientation="horizontal">
        <TextView
            android:gravity="center_vertical"
            android:id="@+id/et_house_type"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:paddingLeft="@dimen/mydp13"
            android:background="@android:color/transparent"
            android:hint="选择包裹类型"
            android:textColor="#666666"
            android:textSize="@dimen/mysp12" />
        <ImageView
            android:id="@+id/iv_icon_a"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginRight="@dimen/mydp8"
            android:clickable="true"
            android:src="@mipmap/xxia" />
    </LinearLayout>
    <LinearLayout
        android:layout_marginTop="@dimen/mydp13"
        android:layout_gravity="center_horizontal"
        android:layout_width="@dimen/mydp301"
        android:layout_height="@dimen/mydp40"
        android:background="@drawable/bg_select_ss"
        android:orientation="horizontal">
        <TextView
            android:gravity="center_vertical"
            android:id="@+id/et_house_type_a"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:paddingLeft="@dimen/mydp13"
            android:background="@android:color/transparent"
            android:hint="正在本站"
            android:textColor="#666666"
            android:textSize="@dimen/mysp12" />
        <ImageView
            android:id="@+id/iv_icon_b"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginRight="@dimen/mydp8"
            android:clickable="true"
            android:src="@mipmap/xxia" />
    </LinearLayout>
    <LinearLayout
        android:layout_marginTop="@dimen/mydp14"
        android:layout_gravity="center_horizontal"
        android:layout_width="@dimen/mydp301"
        android:layout_height="@dimen/mydp40"
        android:background="@drawable/bg_select_ss"
        android:orientation="horizontal">
        <TextView
            android:gravity="center_vertical"
            android:id="@+id/et_start_time"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:paddingLeft="@dimen/mydp13"
            android:background="@android:color/transparent"
            android:hint="选择启始日期"
            android:textColor="#666666"
            android:textSize="@dimen/mysp12" />
        <ImageView
            android:id="@+id/iv_icon_c"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginRight="@dimen/mydp8"
            android:clickable="true"
            android:src="@mipmap/rq" />
    </LinearLayout>
    <LinearLayout
        android:layout_marginTop="@dimen/mydp14"
        android:layout_gravity="center_horizontal"
        android:layout_width="@dimen/mydp301"
        android:layout_height="@dimen/mydp40"
        android:background="@drawable/bg_select_ss"
        android:orientation="horizontal">
        <TextView
            android:gravity="center_vertical"
            android:id="@+id/et_stop_time"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:paddingLeft="@dimen/mydp13"
            android:background="@android:color/transparent"
            android:hint="选择终止日期"
            android:textColor="#666666"
            android:textSize="@dimen/mysp12"/>
        <ImageView
            android:id="@+id/iv_icon_d"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginRight="@dimen/mydp8"
            android:clickable="true"
            android:src="@mipmap/rq" />
    </LinearLayout>
    <TextView
        android:id="@+id/tv_demand"
        android:layout_width="match_parent"
        android:layout_height="@dimen/mydp41"
        android:layout_marginLeft="@dimen/mydp30"
        android:layout_marginRight="@dimen/mydp30"
        android:layout_marginTop="@dimen/mydp40"
        android:background="@drawable/bg_select"
        android:clickable="true"
        android:gravity="center"
        android:text="查 询"
        android:textColor="#ffffff"
        android:textSize="@dimen/mysp14" />
</LinearLayout>